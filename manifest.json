{
    "name": "XHRHook",
    "version": "1.0",
    "description": "Hook all XML HTTP Requests (all tabs)",
    "permissions": ["tabs", "https://www.youtube.com/", "https://www.netflix.com/"],
    "background": {
      "scripts": ["patch.js", "XMLHttpRequestHOOK.js"],
      "persistent": false
    },
    "manifest_version": 2
  }